{
    "name": "projeto",
    "version": 2,
    "builds": [
      {
        "src": "home.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/home.js" 
      },
      {
        "src": "/(.*)", 
        "dest": "/home.js"
      }
    ]
  }
  